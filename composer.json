{
    "name": "wptownhall/login-me-now",
    "autoload": {
        "psr-4": {
            "LoginMeNow\\": "app",
            "LoginMeNow\\Abstracts\\": "app/Common/Abstracts",
            "LoginMeNow\\Traits\\": "app/Common/Traits",
            "LoginMeNow\\Model\\": "app/Models",
            "LoginMeNow\\Routes\\": "app/Routes",
            "LoginMeNow\\GoogleLogin\\": "app/Modules/GoogleLogin",
            "LoginMeNow\\FacebookLogin\\": "app/Modules/FacebookLogin",
            "LoginMeNow\\OnetimeNumber\\": "app/Modules/OnetimeNumber",
            "LoginMeNow\\UserSwitching\\": "app/Modules/UserSwitching",
            "LoginMeNow\\LoginLink\\": "app/Modules/LoginLink",
            "LoginMeNow\\BrowserTokenLogin\\": "app/Modules/BrowserTokenLogin",
            "LoginMeNow\\AdvanceSharing\\": "app/Modules/AdvanceSharing",
            "LoginMeNow\\LoginForm\\": "app/Modules/LoginForm"
        }
    },
    "authors": [
        {
            "name": "yes"
        }
    ],
    "require": {
        "php": ">=7.4",
        "firebase/php-jwt": "^6.3",
        "google/apiclient": "^2.12.1",
        "heymehedi/utils": "^1.1"
    },
    "scripts": {
        "build": "composer --no-plugins --no-interaction install --no-scripts --no-dev",
        "pre-autoload-dump": "Google\\Task\\Composer::cleanup"
    },
    "extra": {
        "google/apiclient-services": [
            "Oauth2"
        ]
    }
}
